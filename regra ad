  aCaracter = aNomFun;
  TamanhoAlfa(aCaracter,nTam);
  nConta = 1;
  i = 1;
  Enquanto (nConta <= nTam)
  INICIO
    aCaracter = aNomFun;
    CopiarAlfa (aCaracter, nConta, 1);
    Se (aCaracter <> " ")
    INICIO
      Se (i=1)
        aNom1 = aNom1 + aCaracter;
      Se (i=2)
        aNom2 = aNom2 + aCaracter;
      Se (i=3)
        aNom3 = aNom3 + aCaracter;
      Se (i=4)
