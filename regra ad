  aNomFun = xNomFun;
  Se ((xNomCom <> "") e (xNomCom <> " "))
    aNomFun = xNomCom;
  
  @=== Senha===================@
  ConverteMascara (1, xNumCpf, aNumCpf, "99999999999");
  CopiarAlfa (aNumCpf, 1, 4);
  
