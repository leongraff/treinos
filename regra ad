      Se (i=9)
        aNom9 = aNom9 + aCaracter;
      Se (i=10)
        aNom10 = aNom10 + aCaracter;
        
      nConta++;
    FIM;
    Senao
    INICIO
      nConta++;
      i++;
    FIM;
  FIM;
  
  @=== Monta Primeiro Nome e Ultimo Nome ===@
  xPrimeiroNome = aNom1;
  
  Se (i = 2)
    xUltimoNome = aNom2;
  Se (i = 3)
