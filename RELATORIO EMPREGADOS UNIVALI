  @ HORARIO_TRABALHO  @
  Ccursor.sql"select R004HOR.CodHor from R004HOR ";
  Ccursor.AbrirCursor();
  Enquanto (Ccursor.Achou)
  Inicio
    xCodHor = Ccursor.CodHor;
    ConverteMascara (1, Ccursor.CodHor, aaux, "999999");
    AlteraControle ("aCodHor", "Descrição", aaux);
    ccursorx.sql "select CodHor,SeqMar,HorBat from R004MHR  where CodHor = :xCodHor";
    ccursorx.abrircursor();
    zcontmar = 1;
    enquanto(ccursorx.achou)
     { 
      zcontmar++;
      ccursorx.proximo();
     };
    ccursorx.fecharcursor();
    ccursorx.abrircursor();
    enquanto(ccursorx.achou)
    {
      se (zcontmar < 4)
       {
        se(ccursorx.SeqMar = 1)
        {
          ConverteMascara (4, ccursorx.HorBat, aaux, "HH:MM");
          AlteraControle ("entrada", "Descrição", aaux);
          zintauto = ccursorx.HorBat + 2;
          ConverteMascara (4, zintauto, aaux, "HH:MM");
          AlteraControle ("Saiinter", "Descrição", aaux);
          zintauto = zintauto + 1;
          ConverteMascara (4, zintauto, aaux, "HH:MM");
          AlteraControle ("Voltainter", "Descrição", aaux);
        }
        se(ccursorx.SeqMar = 2)
        {
         ConverteMascara (4, ccursorx.HorBat, aaux, "HH:MM");
         AlteraControle ("saida", "Descrição", aaux);
        } 
       };
      senao 
       { 
        se(ccursorx.SeqMar = 1)
        {
          ConverteMascara (4, ccursorx.HorBat, aaux, "HH:MM");
          AlteraControle ("entrada", "Descrição", aaux);
        }
        se(ccursorx.SeqMar = 2)
        {
         ConverteMascara (4, ccursorx.HorBat, aaux, "HH:MM");
         AlteraControle ("Saiinter", "Descrição", aaux);
        }
        se(ccursorx.SeqMar = 3)
        {     
         ConverteMascara (4, ccursorx.HorBat, aaux, "HH:MM");
         AlteraControle ("Voltainter", "Descrição", aaux);
        }
        se(ccursorx.SeqMar = 4)
        {
         ConverteMascara (4, ccursorx.HorBat, aaux, "HH:MM");
         AlteraControle ("saida", "Descrição", aaux);
         }
       };  
    ccursorx.proximo();
    };
    ccursorx.fecharcursor();
    Listasecao("Adicional_HORARIO_TRABALHO");
    Ccursor.Proximo();
  Fim;
  Ccursor.FecharCursor();
  
  @  ESCALA_TRABALHO   @
  Ccursor.sql"select R006ESC.CodEsc,R006ESC.nomesc, R006ESC.HorMes from R006ESC ";
  Ccursor.AbrirCursor();
  Enquanto (Ccursor.Achou)
  Inicio
     ncodesc = Ccursor.Codesc; 
     
