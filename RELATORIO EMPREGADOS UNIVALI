                       and r017ghe.codghe = r583ghe.codghe \ 
                       and r017ghe.datfim = :ddatzro \  
                       and r017ghe.codpra = r583rev.codpra \ 
                       and r017ghe.codghe = r583rev.codghe \ 
                       and r583rev.datter >= :ddatsis \ 
