    AlteraControle ("FimPerhfe", "Descrição", aaux);  
    Convertemascara(3,Ccursor.IniFer,aaux,"dd/mm/yyyy"); 
    AlteraControle ("IniFerhfe", "Descrição", aaux); 
    dfimfer = Ccursor.IniFer + Ccursor.Diafer - 1;
    Convertemascara(3,dfimfer,aaux,"dd/mm/yyyy"); 
    AlteraControle ("FimFerhfe", "Descrição", aaux);  
    Convertemascara(1,Ccursor.DiaFer,aaux,"99,99"); 
    AlteraControle ("DiaFerhfe", "Descrição", aaux);
    Convertemascara(1,Ccursor.DiaAbo,aaux,"99"); 
    AlteraControle ("DiaAbohfe", "Descrição", aaux);
    se (ccursor.opc13s = "S")
      Alteracontrole ("Per13","Descrição","50,00");
     senao 
      Alteracontrole ("Per13","Descrição","00,00");
    Listasecao("Adicional_HIST_FERIAS");
    Ccursor.Proximo();
  Fim;
  Ccursor.FecharCursor();
}; 

se(ETipArq = "F")
 {
  @ FOLHA_COLABORADOR@

  definir data ddatcal;
  definir data ddatsis;
  desmontadata (edatref,x,y,z);
  y--;
  se (y = 0)
   {
    y = 12;
    z--;
   }; 
  montadata(x,y,z,ddatcal);
  ddatsis = Datsis;
  Ccursor.sql"Select R046VER.NumEmp, R046VER.NumCad, R046VER.CodCal,  \
                     R044CAL.PerRef, R044CAL.DatPag, R044CAL.TipCal,  \
                     R044CAL.FimCmp                                   \ 
                from R044CAL, R046VER \
               where R044CAL.NumEmp = R046VER.NumEmp \
                 and R044CAL.CodCal = R046VER.CodCal and 1=2 union \
              Select distinct R046VER.NumEmp, R046VER.NumCad, R046VER.CodCal,  \
                     R044CAL.PerRef, R044CAL.DatPag, R044CAL.TipCal, R044CAL.FimCmp  \
                from R044CAL, R046VER  \
               where R044CAL.NumEmp = R046VER.NumEmp \
                 and R044CAL.CodCal = R046VER.CodCal \
                 and R044CAL.PerRef = :dDatcal";

                   @  and R044CAL.DATPAG = :ddatsis@


  Ccursor.AbrirCursor();
  Enquanto (Ccursor.Achou)
  Inicio
    Intparaalfa(Ccursor.NumEmp,aaux);
    ConverteMascara (1, Ccursor.NumEmp, aaux, "999"); 
    AlteraControle ("NumEmpFF", "Descrição", aaux);
    Intparaalfa(Ccursor.NumCad,aaux);
    ConverteMascara (1, Ccursor.NumCad, aaux, "99999999"); 
    AlteraControle ("NumCadFF", "Descrição", aaux);
    Intparaalfa(Ccursor.CodCal,aaux);
    ConverteMascara (1, Ccursor.CodCal, aaux, "999999"); 
    AlteraControle ("CodCalff", "Descrição", aaux);
    Convertemascara(3,Ccursor.FimCmp,aaux,"dd/mm/yyyy"); 
    se ( aaux = "00/00/0000")
      Convertemascara(3,Ccursor.DatPag,aaux,"dd/mm/yyyy");
    AlteraControle ("PerRefFF", "Descrição", aaux);   
    Convertemascara(3,Ccursor.DatPag,aaux,"dd/mm/yyyy"); 
    AlteraControle ("DatPagFF", "Descrição", aaux);
    se(  Ccursor.TipCal = 11)
      AlteraControle ("TipCalff", "Descrição", "03");
    se(  Ccursor.TipCal = 12)
      AlteraControle ("TipCalff", "Descrição", "09");
    se(  Ccursor.TipCal = 91)
      AlteraControle ("TipCalff", "Descrição", "01");
    se(  Ccursor.TipCal = 41)
      AlteraControle ("TipCalff", "Descrição", "02"); 
    se(  Ccursor.TipCal = 31)
      AlteraControle ("TipCalff", "Descrição", "05"); 
    se(  Ccursor.TipCal = 32)
      AlteraControle ("TipCalff", "Descrição", "07"); 
    se(  Ccursor.TipCal = 15)
       AlteraControle ("TipCalff", "Descrição", "10"); 
    se(  Ccursor.TipCal = 92)
       AlteraControle ("TipCalff", "Descrição", "11");   

   nNumEmp = Ccursor.NumEmp; 
   ntipcol = 1;
   nNumcad = Ccursor.Numcad;
   nCodcal = Ccursor.Codcal;
   
   @vantagens @ 
   xvantagem = 0;
   Ccursorx.sql "Select R046VER.NumEmp, R046VER.NumCad, R046VER.CodCal, r046ver.valeve  \
              from R046VER,  R008EVC  \
             where R008EVC.CodTab = R046VER.Tabeve  \
               and R008EVC.CodEve = R046VER.CodEve and 1=2 union \
               Select R046VER.NumEmp, R046VER.NumCad, R046VER.CodCal, sum(r046ver.valeve)  \
              from R046VER,  R008EVC  \
             where R008EVC.CodTab = R046VER.Tabeve  \
               and R008EVC.CodEve = R046VER.CodEve \
               and R046VER.NumEmp = :nNumEmp \
               and R046VER.tipcol = :ntipcol \
               and R046VER.Numcad = :nNumcad \
               and R046VER.CodCal = :nCodcal \
               and R008EVC.TipEve in(1,2) group by R046VER.NumEmp, R046VER.NumCad, R046VER.CodCal ";
   Ccursorx.abrircursor();
   se(Ccursorx.achou)
   inicio 
     xvantagem = Ccursorx.valeve;    
   Fim;
   Convertemascara(1,xvantagem,aaux,"999999999,99");             
   AlteraControle ("Totvanff", "Descrição", aaux);
   Ccursorx.Fecharcursor(); 
   aaux =  "";
   @Descontos@
    Ccursorx.sql "Select R046VER.NumEmp, R046VER.NumCad, R046VER.CodCal, r046ver.valeve  \
              from R046VER,  R008EVC  \
             where R008EVC.CodTab = R046VER.Tabeve  \
               and R008EVC.CodEve = R046VER.CodEve and 1=2 union \
               Select R046VER.NumEmp, R046VER.NumCad, R046VER.CodCal, sum(r046ver.valeve)  \
              from R046VER,  R008EVC  \
             where R008EVC.CodTab = R046VER.Tabeve  \
               and R008EVC.CodEve = R046VER.CodEve \
               and R046VER.NumEmp = :nNumEmp \
               and R046VER.tipcol = :ntipcol \
               and R046VER.Numcad = :nNumcad \
               and R046VER.CodCal = :nCodcal \
               and R008EVC.TipEve in(3) group by R046VER.NumEmp, R046VER.NumCad, R046VER.CodCal ";
   Ccursorx.abrircursor();
   se(Ccursorx.achou)
   inicio 
    xdesconto = Ccursorx.valeve;     
   Fim;
   Convertemascara(1,xdesconto,aaux,"999999999,99");             
   AlteraControle ("Totdesff", "Descrição", aaux);
   Ccursorx.Fecharcursor(); 
   aaux =  "";
   xliquido = xvantagem - xdesconto;  
   Convertemascara(1,xliquido,aaux,"999999999,99"); 
   AlteraControle ("LiqFolff", "Descrição", aaux);
   Listasecao("Adicional_FOLHA_COLABORADOR");
   Ccursor.Proximo();
  Fim;
  Ccursor.FecharCursor();

  @LINHA_FOLHA_COLAB@

  Ccursor.sql"Select R046VER.NumEmp, R046VER.NumCad, R046VER.CodEve, \
                     R046VER.RefEve, R046VER.ValEve, R044CAL.CodCal  \
                From R046VER,R044CAL  \
                where  R046VER.NUMEMP=R044CAL.NUMEMP AND \
                       R046VER.CODCAL=R044CAL.CODCAL AND \
                       R044CAL.PERREF=:ddatcal";
                       
                         @  and R044CAL.DATPAG = :ddatsis@

  Ccursor.AbrirCursor();
  Enquanto (Ccursor.Achou)
  Inicio
    Intparaalfa(Ccursor.NumEmp,aaux);
    ConverteMascara (1, Ccursor.NumEmp, aaux, "999");
     AlteraControle ("NumEmpFl", "Descrição", aaux);
     Intparaalfa(Ccursor.NumCad,aaux);
     ConverteMascara (1, Ccursor.NumCad, aaux, "99999999");
     AlteraControle ("NumCadFl", "Descrição", aaux);
     Intparaalfa(Ccursor.CodCal,aaux);
     ConverteMascara (1, Ccursor.CodCal, aaux, "999999");
     AlteraControle ("CodCalFl", "Descrição", aaux);
     ConverteMascara (1, Ccursor.CodEve, aaux, "9999");
     AlteraControle ("CodEveFF1", "Descrição", aaux);
     Convertemascara(1,Ccursor.RefEve,aaux,"9999999,99"); 
     AlteraControle ("RefEveFl", "Descrição", aaux);
     Convertemascara(1,Ccursor.ValEve,aaux,"999999999,99"); 
     AlteraControle ("ValEveFl", "Descrição", aaux);
     Listasecao("Adicional_LINHA_FOLHA_COLAB");
     Ccursor.Proximo();
  Fim;
  Ccursor.FecharCursor();
}; 

