Definir cursor Ccursor; 
Definir cursor cCursorx;
Definir data ddatzro;
Definir alfa ETipArq;
Definir alfa aaux;
Definir data DatPERFEF;
definir data dinicmp;
definir data dfimcmp;
definir data edatref;
edatref = DatSis;   @ Inserido para tratamento do PA @
Desmontadata(edatref,x,y,z);
montadata(1,y,z,edatref);
dinicmp = edatref;
dfimcmp = edatref;
ultimodia(dfimcmp);
ddatzro = 0;

se(ETipArq = "C")
{
  @ Cargo@
  Ccursor.sql"select CodCar,TitRed,TitCar,CodCb2,SisCar from R024CAR where Codcar like 'C%' and length(CodCar)<6";
  Ccursor.AbrirCursor();
  Enquanto (Ccursor.Achou)
  Inicio
    definir alfa aaux2;
    aaux2 = Ccursor.CodCar;
    aaux =  Ccursor.CodCar; 
    copiaralfa(aaux,1,1);
    copiaralfa(aaux2,2,5);
    aaux = aaux+"."+aaux2;
    AlteraControle ("cCodCar", "Descrição", aaux);
    AlteraControle ("cTitCar", "Descrição", Ccursor.Titcar);
    AlteraControle ("cTitRed", "Descrição", Ccursor.titRed);
    AlteraControle ("cCodCBO", "Descrição", Ccursor.Codcb2);
    aaux ="0";
    se(Ccursor.siscar = 1)
    aaux ="1";
    se(Ccursor.siscar = 2)
    aaux ="2"; 
    se(Ccursor.siscar = 3)
    aaux ="3";  
    AlteraControle ("ctipCar", "Descrição", aaux);
    @ D(direta); I(indireta); A(Administrativa); S(Serviços); O(Outros); PADRÃO  = A   @
    aaux ="A"; 
    AlteraControle ("cMaoObra", "Descrição", aaux);
    Listasecao("Adicional_cargo");
    Ccursor.Proximo();
  Fim;
  Ccursor.FecharCursor();
  
  @ Função @
  Ccursor.sql"select CodCar,TitRed,TitCar,CodCb2,SisCar from R024CAR where Codcar like 'C%' and length(CodCar)<6";
  Ccursor.AbrirCursor();
  Enquanto (Ccursor.Achou)
  Inicio  
