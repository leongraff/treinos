se(ETipArq = "E")
 {
  @ COLABORADOR (R034FUN / R034CPL)@
  
  DatPERFEF = edatref;
  
  Ccursor.sql"Select R034FUN.NumEmp,R034FUN.NumCad,R034FUN.NomFun,R034FUN.SitAfa,R034FUN.GraIns,R034FUN.ESTPOS,R034FUN.POSTRA, \
   R034fun.DatNas,R034FUN.NumCpf,R034FUN.DefFis,R034FUN.DatAdm,R034FUN.TipAdm,R034FUN.TipCon, R034FUN.TIPSEX,   \
   R034FUN.NumCtp,R034FUN.SerCtp,R034FUN.EstCtp,R034FUN.DexCtp,R034FUN.NumPis,R034FUN.DcdPis, \
   R034CPL.EMICID,R034CPL.CiDEMI,R034CPL.ESTCID,R034CPL.DEXCID,R034CPL.ZONELE,R034CPL.SECELE,R034CPL.NUMELE, \
   R034CPL.NUMCNH,R034CPL.CATCNH,R034CPL.DATCNH,R034CPL.VENCNH,R034CPL.NUMRES,R034CPL.CATRES, \
   r034fun.codpes,R074CID.NomCid, R034FUN.NumCpf,R034CPL.NumCid,R034CPL.EndRua, R034CPL.EndNum, \
   R034CPL.EndCpl, R074BAI.NomBai, R034CPL.EndCep,R034CPL.NumTel,R034CPL.EmaPar,R034CPL.CodEst, \
   R034fun.EstCiv,R034CPL.PaiNas,R034FUN.DatChe,R074PAI.NOMPAI,R034CPL.DDDTEL,r034cpl.numram,r034cpl.nmtel2,  \ 
   R034CPL.EMACOM,R034FUN.DEFFIS, R034FUN.MODPAG,R034FUN.CODBAN,R034FUN.CODAGE,R034FUN.CONBAN, R034FUN.DIGBAN,\
   R034FUN.CODFIL, R034FUN.CODCCU,R034FUN.CODCAR,R034FUN.VALSAL,R034FUN.CodEsc,R034FUN.CODTMA,R034FUN.CONRHO,R034FUN.NUMCRA, \ 
   r034cpl.ficreg, r034fun.codvin, r034fun.tipsal,r034fun.raccor,r034fun.outcon,r034fun.outtet,r034fun.socsin,r034fun.pagsin, \
   r034fun.perins, r034fun.perper,r034cpl.ccinas,R006ESC.HORMES \
  from R034fun, r034CPL,R074CID,R074BAI,R074PAI,R006ESC \
              where R074BAI.codcid = r034CPL.codcid \ 
                and R074BAI.codBai = r034CPL.codBai \
                and R074CID.codcid = r034CPL.codcid \ 
                and R074PAI.CODPAI = r034cpl.codpai \
                and R034fun.numemp = r034CPL.numemp \
                and R034fun.tipcol = r034CPL.tipcol \
                and R034fun.numcad = r034CPL.numcad \
                AND R034FUN.CodEsc = R006ESC.CodEsc \
                and R034fun.Tipcol = 1              \               
                and ((R034FUN.SITAFA<>7 OR (R034FUN.SITAFA=7 AND R034FUN.DATAFA+60>=:DatPERFEF)) \
                      OR (r034fun.sitafa=7 and exists (select 1 from r046ver xx,r044cal zz \
                                                                           where  xx.numemp=r034fun.numemp and xx.tipcol=r034fun.tipcol and \
                                                                                  xx.numcad=r034fun.numcad and zz.numemp=xx.numemp and \
                                                                                  zz.codcal=xx.codcal and zz.perref=:DatPERFEF)))";

  Ccursor.AbrirCursor();
  Enquanto (Ccursor.Achou)
  Inicio
     z_numemp = ccursor.numemp;
