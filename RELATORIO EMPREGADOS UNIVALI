                     r583ghe , \ 
                     r583rev , \ 
                     r583lau  \ 
                 where r034fun.postra = r017pos.postra \ 
                       and r034fun.estpos = r017pos.estpos \  
                       and r017pos.estpos = r017ghe.estpos \ 
                       and r017pos.postra = r017ghe.postra \  
