  @ HORARIO_TURMA @
  Ccursor.sql"select R006ESC.CodEsc,R006TMA.CodTma,R004HOR.CODHOR,r006hor.seqreg,R006TMA.DATBAS  \
                 from R006ESC,R006TMA , R004HOR, r006hor                                          \
                where R006ESC.CodEsc = R006TMA.CodEsc                                              \
                  and R004HOR.CODHOR = r006hor.codhor                                               \
                  and r006hor.CodEsc=R006ESC.CodEsc order by R006ESC.CodEsc,R006TMA.CodTma,R004HOR.CODHOR,r006hor.seqreg";
  Ccursor.AbrirCursor();
  Enquanto (Ccursor.Achou)
  Inicio
          
     ConverteMascara (1, Ccursor.seqreg, aaux, "999");
     AlteraControle ("HCodSeq", "Descrição", aaux);
     ConverteMascara (1, Ccursor.Codesc, aaux, "999999");
     AlteraControle ("HCodesc", "Descrição", aaux);
     ConverteMascara (1, Ccursor.CodTma, aaux, "999999");
     AlteraControle ("Hcodtur", "Descrição", aaux);
     ConverteMascara (1, Ccursor.CODHOR, aaux, "999999");
     AlteraControle ("hcodHor", "Descrição", aaux);
     
     @RetornaDiaSemana(Ccursor.DATBAS,aDiaSem);     @
     Desmontadata(Ccursor.DATBAS,B1,B2,B3);
     se ( B2<3){
       b3=b3-1;
       b2=b2+12;
     }
     total=B1+(2*B2);
     parcial=(3*(B2+1))/5;
     total=total+Truncar(parcial);
     total=total+B3;
     parcial=B3/4;
     total=total+Truncar(parcial);
     parcial=B3/100;
     total=total-Truncar(parcial);
     parcial=B3/400;
     total=total+Truncar(parcial);
     total=total+2;
     RestoDivisao(total,7,nDiaSem);
     RestoDivisao(Ccursor.seqreg, 7, nSeq);
     ConverteMascara (1, nSeq-1+nDiaSem , aaux, "99"); 
     AlteraControle ("hdia", "Descrição", aaux); 
     
    Listasecao("Adicional_Hor_turma");     
    
    Ccursor.Proximo();
  Fim;
  Ccursor.FecharCursor();
  
