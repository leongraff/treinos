  @ CENTRO_RESULTADO (R018CCU)  @
  Ccursor.sql"Select NumEmp,FilCtb,CodCcu,NomCcu,CodRat from R018CCU WHERE  FilCtb>0 and \
                     exists (select 1 from r038hcc where r038hcc.numemp=r018ccu.numemp and \
                                                         r038hcc.codccu=r018ccu.codccu) ";
  Ccursor.AbrirCursor();
  Enquanto (Ccursor.Achou)
  Inicio
     
     ConverteMascara (1, Ccursor.NumEmp, aaux, "999");
     AlteraControle ("NumEmpc", "Descrição", aaux);
     ConverteMascara (1, 1, aaux, "9999");
     AlteraControle ("CodFilc", "Descrição", aaux);
     aaux = Ccursor.CodCcu;
     convertemascara(5,0,aaux,"999999999999999");
     AlteraControle ("codccuc", "Descrição", aaux);
     AlteraControle ("CodRatc", "Descrição", aaux);
     aaux =  Ccursor.NomCcu;
     copiaralfa(aaux,1,35);
     AlteraControle ("NomCcuc", "Descrição", aaux);  
     Listasecao("Adicional_CENTRO_RESULTADO");
     
     ConverteMascara (1, Ccursor.NumEmp, aaux, "999");
     AlteraControle ("NumEmpc", "Descrição", aaux);
     ConverteMascara (1, 2, aaux, "9999");
     AlteraControle ("CodFilc", "Descrição", aaux);
     aaux = Ccursor.CodCcu;
     convertemascara(5,0,aaux,"999999999999999");
     AlteraControle ("codccuc", "Descrição", aaux);
     AlteraControle ("CodRatc", "Descrição", aaux);
     aaux =  Ccursor.NomCcu;
     copiaralfa(aaux,1,35);
     AlteraControle ("NomCcuc", "Descrição", aaux);  
     Listasecao("Adicional_CENTRO_RESULTADO");
     Ccursor.Proximo();
  Fim;
  Ccursor.FecharCursor(); 
  @EVENTO_FOLHA (R008EVC)   @  
  Ccursor.sql"Select * from R008EVC WHERE CODTAB=941 AND CODEVE<6000";
  Ccursor.AbrirCursor();
  Enquanto (Ccursor.Achou)
  Inicio
     Intparaalfa(Ccursor.CodTab,aaux);
     AlteraControle ("NumEmpe", "Descrição", "001");
     ConverteMascara (1, Ccursor.CodEve, aaux, "9999");
     AlteraControle ("CodEvee", "Descrição", aaux);
     AlteraControle ("DescEve", "Descrição", Ccursor.DesEve);
     se((Ccursor.TipEve =1)ou(Ccursor.TipEve =2))
     {
      AlteraControle ("aTipEVE", "Descrição", "1");
      AlteraControle ("aTipNat", "Descrição", "P");
     }
     se((Ccursor.TipEve =3))
     {
      AlteraControle ("aTipEVE", "Descrição", "1");
      AlteraControle ("aTipNat", "Descrição", "D");
     }
        se((Ccursor.TipEve =4))
     {
      AlteraControle ("aTipEVE", "Descrição", "1");
      AlteraControle ("aTipNat", "Descrição", "O");
     }
     
   
     AlteraControle ("FatorM", "Descrição", "0,00");
  
  
    Listasecao("Adicional_EVENTO_FOLHA");
      Ccursor.Proximo();
  Fim;
  Ccursor.FecharCursor();
  @RELOGIO_PONTO (R058RLG) @
