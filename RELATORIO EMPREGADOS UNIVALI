     Convertemascara(1,Ccursor.HorMes,aaux,"999,99"); 
     AlteraControle ("HorMes", "Descrição", aaux);
     nqtdhor =0; 
     cCursorx.sql " select CodHor from R006HOR where 1 = 2 union \
                    select count(CodHor) from R006HOR  where CodEsc = :ncodesc and CodHor <9996 "
     cCursorx.abrircursor();
     se(cCursorx.achou)
       nqtdhor = cCursorx.codhor;
     cCursorx.fecharcursor(); 
    Convertemascara(1,nqtdhor,aaux,"999"); 
     AlteraControle ("DiasSema", "Descrição", aaux);   
    Listasecao("Adicional_ESCALA_TRABALHO");
    
    Ccursor.Proximo();
  Fim;
  Ccursor.FecharCursor();
  
   @ TURMA_ESCALA_TRABALHO @
  Ccursor.sql"select R006ESC.CodEsc,R006ESC.nomesc ,R006TMA.CodTma,R006TMA.DatBas from R006ESC,R006TMA \
            where R006ESC. CodEsc = R006TMA.CodEsc";
  Ccursor.AbrirCursor();
  Enquanto (Ccursor.Achou)
  Inicio
    
     ConverteMascara (1, Ccursor.Codesc, aaux, "999999");
     AlteraControle ("Codesc_t", "Descrição", aaux);
     ConverteMascara (1, Ccursor.CodTma, aaux, "999999");
     AlteraControle ("Codturma", "Descrição", aaux);
     AlteraControle ("NomTurma", "Descrição", Ccursor.nomesc);
     Convertemascara(2,Ccursor.DatBas,aaux,"dd/mm/yyyy"); 
     AlteraControle ("DatIniTur", "Descrição", aaux); 
     
    Listasecao("adicional_HORARIO_TURMA");
    
